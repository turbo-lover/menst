<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.woman_calendar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.woman_calendar.activity.LauncherActivity"
            android:screenOrientation="landscape"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.woman_calendar.activity.SettingsActivity"
            android:screenOrientation="landscape"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
            </activity>

        <activity android:name="com.woman_calendar.activity.DayParamActivity"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
            </activity>
        <activity
                android:name="com.woman_calendar.activity.SymptomActivity"
                android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
        </activity>
        <activity android:name="com.woman_calendar.activity.CalendarActivity"
                android:screenOrientation="landscape"/>
        <activity android:name="com.woman_calendar.activity.WeightActivity"
            android:screenOrientation="landscape" />
       <activity android:name="com.woman_calendar.activity.TemperatureActivity"
           android:screenOrientation="landscape" />

    </application>

</manifest>
